{
  "type": "module",
  "name": "@tags/server",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/app.js",
    "start:debug": "cross-env NODE_ENV=production node --inspect dist/app.js",
    "dev": "cross-env NODE_ENV=development SERVER_PROTOCOL=http tsx --tsconfig tsconfig.json --watch src/app.ts",
    "dev:http": "cross-env NODE_ENV=development SERVER_PROTOCOL=http tsx --tsconfig tsconfig.json --watch src/app.ts",
    "dev:https": "cross-env NODE_ENV=development SERVER_PROTOCOL=https tsx --tsconfig tsconfig.json --watch src/app.ts",
    "dev:debug": "cross-env NODE_ENV=development SERVER_PROTOCOL=http tsx --tsconfig tsconfig.json --watch --inspect src/app.ts",
    "dev:debug:https": "cross-env NODE_ENV=development SERVER_PROTOCOL=https tsx --tsconfig tsconfig.json --watch --inspect src/app.ts",
    "build": "tsc --project tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"No tests yet\" && exit 0",
    "clean": "rm -rf dist"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.1",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tags/eslint-config": "workspace:*",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.8.1",
    "@types/nodemailer": "^7.0.2",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "cross-env": "^10.0.0",
    "eslint": "^9.36.0",
    "globals": "^15.15.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  }
}
